{
  "name": "bytic/locale",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/locale",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Locale\\": "src"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
      "src/functions/application_namespaced.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Locale\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-intl": "*",
    "bytic/helpers": "^1.0|^2.0"
  },
  "require-dev": {
    "bytic/container": "^1.0|^2.0",
    "bytic/phpqatools": "^1.0",
    "mockery/mockery": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
